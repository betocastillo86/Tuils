@model CatalogPagingFilteringModel.ManufacturerFilterModel
@using Nop.Web.Models.Catalog;

@if (Model.Enabled)
{
    if (Model.NotFilteredItems != null && Model.NotFilteredItems.Count > 0)
    {
        <ul class="product-filters">
            <div class="filter-title">@T("filtering.manufacturerfilter")</div>

            @foreach (var item in Model.NotFilteredItems)
            {
                <li><a href="@item.FilterUrl"><span class="namefil">@item.Name</span> <span class="fil_cant">@item.NumOfProducts</span></a></li>
            }
        </ul>
    }
}