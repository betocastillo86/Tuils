@model CatalogPagingFilteringModel.CategoryFilterModel
@using Nop.Web.Models.Catalog;
@if (Model.Enabled)
{ 
    if (Model.AlreadyFilteredItems.Count > 0)
    {
        foreach (var item in Model.AlreadyFilteredItems)
        {
            <li>@(item.Name) <a href="@Model.RemoveFilterUrl" rel="nofollow"><span class="icon-close"></span></a></li>
        }
    }
}