@model CatalogPagingFilteringModel.BikeReferenceFilterModel
@using Nop.Web.Models.Catalog;

@if (Model.Enabled)
{
    if (Model.NotFilteredItems != null && Model.NotFilteredItems.Count > 0)
    {
        <ul class="product-filters">
            <div class="filter-title">@T("Filtering.BikeReferenceFilter")</div>

            @foreach (var item in Model.NotFilteredItems)
            {
                <li><a href="@item.FilterUrl">@item.Name <span class="fil_cant">@item.NumOfProducts</span></a></li>
            }
        </ul>
    }
}

