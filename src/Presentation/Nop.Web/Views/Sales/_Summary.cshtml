
<script id="templateSummary" type="text/template">
    <div>
        <%
        _.each(obj.Properties, function(element, index){  %>
            <div>{{element.name}}: {{element.value}}</div>
        <% }); %>

        IMAGENES

        <%
        _.each(obj.Images, function(element, index){   %>
        <div style="width: 100px; height: 100px; border: solid 1px; margin: 3px; float: left">
            <img width="100%" height="100%" src="{{element.src}}"  />
        </div>
        <% }); %>
        <div style="clear:both"></div>
    </div>
    
    <div>
    <input id="chkConditions" type="checkbox" /> Aceptar terminos y condiciones
    </div>
    
    <a class="btnBack">@T("PublishProduct.BackStep")</a>

    

    <a class="btnFinish">@T("PublishProduct.FinishStep")</a>
</script>