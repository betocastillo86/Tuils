@model Nop.Web.Models.Catalog.SimilarSearchesModel

<div class="similarSearch">
    <span title="@Model.TitleOfTitle">@Model.Title</span>
    <ul>
        @foreach (var item in Model.Searches)
        {
            <a href="@Url.Action("Search", new { q = item, query = string.Empty })" title="@T("similarSearches.suggest.title", item)"><li>@item,</li></a>
        }
    </ul>
</div>

